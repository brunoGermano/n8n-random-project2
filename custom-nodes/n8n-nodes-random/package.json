{
  "name": "n8n-nodes-random",
  "version": "0.1.0",
  "description": "Um nó customizado para n8n que gera números aleatórios usando a API Random.org",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Bruno",
    "email": "brunogermano3@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brunoGermano/n8n-random-project2.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc -b && gulp build:icons",
    "dev": "gulp dev",
    "lint": "eslint nodes credentials --ext .ts --fix",
    "lint:styles": "prettier --write \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-core": "^1.85.4",
    "n8n-workflow": "^1.85.4",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
